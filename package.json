{
  "name": "climatebert.ai",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "format": "prettier --write .",
    "fmt": "pnpm lint && pnpm format",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.4.1",
    "@heroicons/react": "1.0.4",
    "@hookform/resolvers": "2.8.2",
    "@prisma/client": "3.3.0",
    "autoprefixer": "10.3.7",
    "classnames": "2.3.1",
    "postcss": "8.3.11",
    "react": "alpha",
    "react-animated-number": "0.4.4",
    "react-dom": "alpha",
    "react-hook-form": "7.17.5",
    "tailwindcss": "2.1.2",
    "zod": "3.10.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.3.4",
    "@tailwindcss/typography": "0.4.1",
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.31",
    "blitz": "0.41.1",
    "eslint": "7.32.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.4.1",
    "prettier-plugin-prisma": "0.15.0",
    "pretty-quick": "3.1.1",
    "preview-email": "3.0.5",
    "prisma": "3.3.0",
    "typescript": "~4.3"
  },
  "private": true
}
